    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="OrdersMap">
    
    </select>

    <!--新增数据-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
    
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
    
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
    
    </delete>

</mapper>

